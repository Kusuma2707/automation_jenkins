pipeline {
    agent any

    stages {
        stage('Checkout') {
    steps {
        git branch: 'devlop', url: 'https://github.com/Kusuma2707/automation_jenkins.git'
    }
}


        stage('Build') {
            steps {
                sh 'chmod +x hello.sh'
                sh './hello.sh'
            }
        }
    }
}
